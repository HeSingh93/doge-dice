<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<?import javafx.scene.image.ImageView?>
<VBox id="helpWindow"
      stylesheets="@/css/helpWindow.css"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.dogedice.controllers.HelpWindow">

  <Label id="header" text="HOW TO PLAY"/>
  <Label fx:id="text" id="text" wrapText="true"/>

  <ImageView id="backButton"
  layoutY="185"
  preserveRatio="true"
  fitWidth="40"
  onMouseClicked="#backIconClicked"/>
</VBox>


